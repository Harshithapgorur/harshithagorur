package com.xworkz.java.boot;
import com.xworkz.java.gorur.VanityBag;

public class VanityBagRunner {

	public static void main(String[] args) {
		String[] colors= {"pink", "blue"};
		String[] bagscompany= {" Runner-up", "Samsonite"};
		String [] items= {"lipstic", "powder"};
		String[] ownerNames= {"you", "me"};
		long[] ownersNumbers= {78198089990l, 98202801837l};
		
		VanityBag vanityBag= new VanityBag(null, null, 0, false, colors, items, bagscompany, ownerNames, ownersNumbers, items, ownersNumbers);
		
		
		vanityBag.display();
		 
		
	}

}
